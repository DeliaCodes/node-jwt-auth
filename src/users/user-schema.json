{
  "type": "object",
  "removeAdditional": true,
  "required": [
    "first_name",
    "last_name",
    "email",
    "password"
  ],
  "properties": {
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "format": "password",
      "pattern": "/^[^\\s]$/",
      "minLength": 2,
      "maxLength": 72
    }
  }
}
